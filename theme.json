	"styles": {
		"blocks": {
			"core/column": {
				"border": {
					"bottom": {
						"color": "var(--wp--custom--border--color)",
						"style": "var(--wp--custom--border--style)",
						"width": "var(--wp--custom--border--width)"
					},
					"right": {
						"color": "var(--wp--custom--border--color)",
						"style": "var(--wp--custom--border--style)",
						"width": "var(--wp--custom--border--width)"
					},
					"top": {
						"color": "var(--wp--custom--border--color)",
						"style": "var(--wp--custom--border--style)",
						"width": "var(--wp--custom--border--width)"
					}
				},
				"spacing": {
					"blockGap": "var(--wp--custom--border--padding)",
					"margin": {
						"bottom": "calc( var(--wp--custom--border--width) *-1) !important",
						"right": "calc( var(--wp--custom--border--width) *-1) !important",
						"top": "calc( var(--wp--custom--border--width) *-1) !important"
					},
					"padding": {
						"bottom": "var(--wp--custom--border--padding)",
						"left": "var(--wp--custom--border--padding)",
						"right": "var(--wp--custom--border--padding)",
						"top": "var(--wp--custom--border--padding)"
					}
				}
			},
			"core/paragraph": {
				"spacing": {
					"padding": {
						"bottom": "0",
						"top": "0"
					}
				}
			},
			"core/query-pagination": {
				"border": {
					"color": "var(--wp--custom--border--color)",
					"style": "var(--wp--custom--border--style)",
					"width": "var(--wp--custom--border--width)"
				},
				"css": ".wp-block-query-pagination{position:relative; z-index:4;}",
				"spacing": {
					"margin": "calc( var(--wp--custom--border--width) *-1) !important"
				}
			},
			"core/query-pagination-next": {
				"border": {
					"color": "var(--wp--custom--border--color)",
					"style": "var(--wp--custom--border--style)",
					"width": "var(--wp--custom--border--width)"
				},
				"spacing": {
					"margin": "calc( var(--wp--custom--border--width) *-1) !important"
				}
			},
			"core/query-pagination-numbers": {
				"border": {
					"color": "var(--wp--custom--border--color)",
					"style": "var(--wp--custom--border--style)",
					"width": "var(--wp--custom--border--width)"
				},
				"spacing": {
					"margin": "calc( var(--wp--custom--border--width) *-1) !important"
				}
			},
			"core/query-pagination-previous": {
				"border": {
					"color": "var(--wp--custom--border--color)",
					"style": "var(--wp--custom--border--style)",
					"width": "var(--wp--custom--border--width)"
				},
				"spacing": {
					"margin": "calc( var(--wp--custom--border--width) *-1) !important"
				}
			},
			"core/social-links": {
				"spacing": {
					"blockGap": "var(--wp--custom--border--padding)"
				}
			}
		},
		"border": {
			"color": "var(--wp--custom--border--color)",
			"style": "var(--wp--custom--border--style)",
			"width": "var(--wp--custom--border--width)"
		},
		"elements": {
			"link": {
				"typography": {
					"textDecoration": "none"
				}
			}
		},
		"spacing": {
			"blockGap": "0px",
			"margin": {
				"bottom": "var(--wp--custom--border--padding)",
				"left": "var(--wp--custom--border--padding)",
				"right": "var(--wp--custom--border--padding)",
				"top": "var(--wp--custom--border--padding)"
			},
			"padding": {
				"bottom": "0px",
				"left": "0px",
				"right": "0px",
				"top": "0px"
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--ibm-plex-mono)"
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header"
